# Globomantics Robot API Configuration
# Copy this file to .env and update values

# Application
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Build Metadata (set by CI/CD)
BUILD_DATE=
COMMIT_SHA=

# Azure Deployment (for Module 3)
AZURE_SUBSCRIPTION_ID=
AZURE_RESOURCE_GROUP=globomantics-robots
AZURE_CONTAINER_REGISTRY=globomanticsacr
AZURE_CONTAINER_APP=robot-api

# Slack Notifications (for Module 3 Orbs demo)
SLACK_WEBHOOK_URL=
SLACK_CHANNEL=#deployments
